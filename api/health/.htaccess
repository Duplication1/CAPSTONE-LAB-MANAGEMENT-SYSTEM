RewriteEngine On

# Route /api/health/computer/{name} to computer.php
RewriteRule ^computer/(.+)$ computer.php [L,QSA]

# Route direct endpoints
RewriteRule ^computers$ computers.php [L,QSA]
RewriteRule ^alerts$ alerts.php [L,QSA]

# Allow access to specific files
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^(.*)$ - [L]